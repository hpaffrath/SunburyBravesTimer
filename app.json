        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.hpaffrath.MyFirstApp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "eda91cd5-5595-410c-890c-c97c792d8391"
      }
    },
    "plugins": [
      "expo-sqlite"
    ]
  }
